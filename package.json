    "rollup-plugin-typescript2": "^0.27.0",
    "source-map-support": "^0.5.9",
    "tslint": "^5.11.0",
    "tslint-language-service": "^0.9.9",
    "tslint-no-unused": "^0.2.0-alpha.1",
    "typescript": "^4.4.4"
  }
