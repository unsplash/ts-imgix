{
  "name": "ts-imgix",
  "main": "./target/index.cjs.js",
  "module": "./target/index.esm.js",
  "typings": "./target/index.d.ts",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/unsplash/ts-imgix.git"
  },
  "version": "0.0.15",
  "scripts": {
